<nav class="navigation fadeIn animated">
    <ul>
        <!-- Loop through the navigation items -->
        {{#foreach navigation}}
            <li class="blog-description nav-{{slug}}{{#if current}} nav-current{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
        <!-- End the loop -->
    </ul>
</nav>
